{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "bundler",
		"noImplicitAny": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": false
	},
	"include": ["src/**/*.js", "src/**/*.svelte", ".storybook/**/*.ts", ".storybook/**/*.js"]
}
